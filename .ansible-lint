---
profile: moderate

warn_list:
  - internal-error
  - command-instead-of-module
  - name[casing]
  - name[template]
  - no-changed-when
  - no-handler
  - no-relative-paths
  - var-naming[no-role-prefix]
  - yaml[line-length]

exclude_paths:
  - .venv/
  - collections/
  - .github/
  - .cache/

offline: true
